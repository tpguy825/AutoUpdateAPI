{
   "name": "pocketmine/phpstan-plugin-config",
   "require": {
      "phpstan/phpstan": "2.1.1",
      "pocketmine/pocketmine-mp": "5.5.0",
      "phpstan/phpstan-strict-rules": "^2.0",
      "phpstan/extension-installer": "^1.0"
   },
   "autoload": {
      "psr-4": {
         "ExamplePlugin\\": "../src"
      }
   },
   "config": {
      "allow-plugins": {
         "phpstan/extension-installer": true
      }
   }
}
